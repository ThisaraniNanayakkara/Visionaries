<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@null"
    android:backgroundTint="#00A4D2"
    tools:context=".AdminPrescribedProducts">


        <TextView
            android:id="@+id/bar_btm"
            android:layout_width="415dp"
            android:layout_height="57dp"
            android:background="#00A4D2"
            android:backgroundTint="#01A4D2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.058"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/bar_top"
            android:layout_width="416dp"
            android:layout_height="65dp"
            android:background="#00A4D2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.043"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/im_menu"
            android:layout_width="45dp"
            android:layout_height="40dp"
            android:layout_marginStart="48dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="48dp"
            android:contentDescription="@string/im_menu"
            app:layout_constraintEnd_toStartOf="@+id/bar_search8"
            app:layout_constraintHorizontal_bias="0.653"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/menu2" />

        <EditText
            android:id="@+id/bar_search8"
            android:layout_width="239dp"
            android:layout_height="39dp"
            android:layout_marginTop="16dp"
            android:backgroundTint="#00a4d2"
            android:ems="10"
            android:gravity="center"
            android:hapticFeedbackEnabled="false"
            android:hint="@string/bar_search4"
            android:inputType="textPersonName"
            android:textAlignment="center"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.601"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="TouchTargetSizeCheck" />

        <ImageView
            android:id="@+id/btn_home1"
            android:layout_width="38dp"
            android:layout_height="38dp"
            android:layout_marginStart="24dp"
            android:layout_marginBottom="12dp"
            android:contentDescription="@string/btn_home"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/home" />

        <ImageView
            android:id="@+id/btn_user1"
            android:layout_width="38dp"
            android:layout_height="40dp"
            android:layout_marginStart="48dp"
            android:layout_marginEnd="24dp"
            android:layout_marginBottom="10dp"
            android:contentDescription="@string/btn_user"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/btn_prescription1"
            app:srcCompat="@drawable/user" />

        <ImageView
            android:id="@+id/btn_prescription1"
            android:layout_width="38dp"
            android:layout_height="40dp"
            android:layout_marginStart="64dp"
            android:layout_marginBottom="8dp"
            android:contentDescription="@string/btn_prescription"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/img_cart_btm1"
            app:srcCompat="@drawable/prescription" />

        <ImageView
            android:id="@+id/img_cart_btm1"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:layout_marginStart="56dp"
            android:layout_marginBottom="8dp"
            android:contentDescription="@string/img_cart_btm"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/btn_home1"
            app:srcCompat="@drawable/cart" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="114dp"
            android:layout_height="91dp"
            android:layout_marginStart="4dp"
            android:layout_marginTop="20dp"
            android:contentDescription="prescriptionprovided"
            app:layout_constraintBottom_toBottomOf="@+id/pres_approved"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/bar_top"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/metformin" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="212dp"
            android:layout_height="172dp"
            android:layout_marginTop="60dp"
            android:contentDescription="TODO"
            app:layout_constraintBottom_toTopOf="@+id/enterpres"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.508"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/pres_approved"
            app:layout_constraintVertical_bias="0.561"
            app:srcCompat="@drawable/image" />

        <Button
            android:id="@+id/btn_submit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="72dp"
            android:background="#000000"
            android:text="@string/btn_ga"
            app:layout_constraintBottom_toTopOf="@+id/bar_btm"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.461"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/pres_approved"
            android:layout_width="255dp"
            android:layout_height="41dp"
            android:layout_marginStart="120dp"
            android:layout_marginTop="52dp"
            android:background="#E3E0E0"
            android:text="@string/pres_provided"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/bar_top" />

        <EditText
            android:id="@+id/enterpres"
            android:layout_width="316dp"
            android:layout_height="44dp"
            android:layout_marginBottom="44dp"
            android:ems="10"
            android:hint="@string/enterpres"
            android:inputType="textPersonName"
            app:layout_constraintBottom_toTopOf="@+id/btn_submit"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.493"
            app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
